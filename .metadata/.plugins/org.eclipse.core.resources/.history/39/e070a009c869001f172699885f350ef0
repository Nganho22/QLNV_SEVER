package QLNVProfile;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.annotation.ComponentScan;

@SpringBootApplication
@EnableMySQLRepositories(basePackages ="QLNVProfileAPI.repository")
@ComponentScan(basePackages = {"QLNVActivityAPI","QLNVActivityAPI.controller","QLNVActivityAPI.model","QLNVActivityAPI.repository"})

public class QlnvProfileApplication {

	public static void main(String[] args) {
		SpringApplication.run(QlnvProfileApplication.class, args);
	}

}
